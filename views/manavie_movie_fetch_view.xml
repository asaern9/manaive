<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="movie_fetch_form_view" model="ir.ui.view">
        <field name="name">movie.form.view</field>
        <field name="model">movie.fetch</field>
        <field name="arch" type="xml">
            <form string="Fetch Movies">
                <sheet>
                    <group>
                        <field name="movie_category" invincible="1" placeholder="Select category..."/>
                    </group>
                    <footer>
                        <button name="fetch_movie_main" string="Fetch" type="object" class="btn-primary"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>